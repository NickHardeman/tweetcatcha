/*Custom event to pass in the ID of the object along with the event*/package com.tweetcatcha.events {      import flash.events.Event;      public class TweetIDEvent extends Event {            public static const TWEETID:String = "gotTweetID";            private var ID:int = 0;            public function get getID():int {         return ID;      }            public function TweetIDEvent(type:String, ID:int){         super(type, true);         this.ID = ID;      }            public override function clone():Event {         return new TweetIDEvent(type, ID);      }	  	     }}