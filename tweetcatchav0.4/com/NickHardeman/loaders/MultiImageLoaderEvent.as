package com.NickHardeman.loaders {	import flash.events.EventDispatcher;	import flash.events.ProgressEvent;	import flash.events.Event;		public class MultiImageLoaderEvent extends ProgressEvent {		public static const PROGRESS:String = "progress";		public static const COMPLETE:String = "complete";				public var name:String;				public var percent:Number = 0;				public var timeElapsed:int = 0;				public var itemsLoaded:int = 0;		public var itemsTotal:int = 0;				public function MultiImageLoaderEvent(name:String, bubbles:Boolean=true, cancelable:Boolean=false ) {			super(name, bubbles, cancelable);			this.name = name;		}				public function updateStats(bytesLoaded:Number, bytesTotal:Number, itemsLoaded:int, itemsTotal:int, timeElapsed:int):void {			this.bytesLoaded = bytesLoaded;			this.bytesTotal = bytesTotal;			this.itemsLoaded = itemsLoaded;			this.itemsTotal = itemsTotal;			this.timeElapsed = timeElapsed;			this.percent = bytesLoaded/bytesTotal;			if (isNaN(percent)) {				percent = 0;			}		}				public override function clone():Event {        	var MILE:MultiImageLoaderEvent =  new MultiImageLoaderEvent(type, bubbles, cancelable);			MILE.updateStats(bytesLoaded, bytesTotal, itemsLoaded, itemsTotal, timeElapsed);			return MILE;     	}				public override function toString():String {			return this.name+" percent : "+percent+"%  "+super.toString();		}			}}