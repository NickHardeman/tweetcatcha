package com.tweetcatcha.visual {	import flash.display.MovieClip;	import flash.events.Event;		import com.tweetcatcha.data.objects.Tweet;		import com.caurina.transitions.*;		public class TweetManager {				private var _mc			:MovieClip;		public var _tweets		:Vector.<TweetClip>;				public function setup( ):void {			_tweets = new Vector.<TweetClip>();		}				public function addTweet($tarX:Number, $tarY:Number, $t:Tweet):void {			var tc:TweetClip = new TweetClip();			//tc.x = 0;			//tc.y = 0;			tc.x = $tarX;			tc.y = $tarY;			_tweets.push( tc );			_mc.addChild(tc);			//tc.addEventListener(Event.ENTER_FRAME, touchTest);			//Tweener.addTween(tc, {x:$tarX, y:$tarY, time:1, transition:"easeOutCubic"});		}				public function touchTest($e:Event):void {			//trace(this._mc.getChildAt(1).x);			for (var i:int = 0; i < this._mc.numChildren; i++) {				//trace(this._mc.getChildAt(i).x);				for (var j:int = 0; j < this._mc.numChildren; j++) {					if (this._mc.getChildAt(i).hitTestObject(this._mc.getChildAt(j))) {						//trace("touching" + this._mc.getChildAt(i).y);						//this._mc.getChildAt(i).y += .1;					}				}			}		}				public function set mc($mc:MovieClip):void {			_mc = $mc;		}	}}