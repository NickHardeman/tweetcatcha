package com.tweetcatcha.nav {	import flash.events.Event;	import flash.events.MouseEvent;	import com.reintroducing.events.CustomEvent;		import flash.net.URLLoader;	import flash.net.URLRequest;		import flash.display.MovieClip;		public class BottomNavMain extends MovieClip {		private var _SCRIPT_LOC		:String = "http://127.0.0.1/Tweetcatcha/getData/";		private var _FILE_NAME		:String = "getAvailableDates.php";		private var _loader			:URLLoader;						public function setup($sLoc:String = ""):void {			if ($sLoc != "") _SCRIPT_LOC = $sLoc;			last24HoursBtn.mouseEnabled = changeDateBtn.mouseEnabled = false;			last24HoursBtn.buttonMode = changeDateBtn.buttonMode = true;			changeDateBtn._txt.text = "Change Date";			//LIST VIEW			listViewBtn.buttonMode = true;			listViewBtn.addEventListener(MouseEvent.MOUSE_DOWN, listViewOnClicked);					}		public function load():void {			_loader = new URLLoader();			_loader.addEventListener(Event.COMPLETE, _onDatesLoaded, false, 0, true);			//_loader.load(_SCRIPT_LOC+_FILE_NAME);		}				private function _onDatesLoaded($e:Event):void {			_loader.removeEventListener(Event.COMPLETE, _onDatesLoaded);			_loader = null;			dispatchEvent($e);		}				public function animateIn():void {					}				//LIST VIEW BTN		public function listViewOnClicked($e:MouseEvent):void {			$e.target.parent.parent._onListViewClicked();		}			}}