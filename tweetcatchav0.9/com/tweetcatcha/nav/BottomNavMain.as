package com.tweetcatcha.nav {	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.EventDispatcher;		import com.reintroducing.events.CustomEvent;	import com.tweetcatcha.data.Constants;		//used as the base class for the bottom Nav .swf	public class BottomNavMain extends EventDispatcher {		private var _mc			:MovieClip;				public function BottomNavMain($mc:MovieClip) {			_mc = $mc;		}				public function setup():void {			//last24HoursBtn.mouseEnabled = changeDateBtn.mouseEnabled = false;			//last24HoursBtn.buttonMode = changeDateBtn.buttonMode = true;			//changeDateBtn._txt.text = "Change Date";			_mc.nav_mc.recW_mc.visible = false;			//LIST VIEW			_mc.nav_mc.listViewBtn.buttonMode = true;			_mc.nav_mc.listViewBtn.mouseChildren = false;			_mc.nav_mc.listViewBtn.addEventListener(MouseEvent.MOUSE_DOWN, listViewOnClicked);					}				public function onStageResize($w:Number, $h:Number):void {			_mc.y = $h - 60;			_mc.bgRec.width = $w;			_mc.nav_mc.x = ($w * .5) - (_mc.nav_mc.recW_mc.width * .5);		}				public function animateIn():void {					}				//LIST VIEW BTN		public function listViewOnClicked($e:MouseEvent):void {			dispatchEvent(new Event( Constants.LIST_VIEW_CLICKED ) );		}				public function get mc():MovieClip {			return _mc;		}			}}