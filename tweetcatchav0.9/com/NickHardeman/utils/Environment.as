package com.NickHardeman.utils {		import flash.system.Capabilities;		public class Environment {				public static function get IS_IN_BROWSER():Boolean {			if (Capabilities.playerType == "ActiveX" || Capabilities.playerType == "PlugIn")  { return true;}			else { return false;}		}				public static function IS_AIR():Boolean {			if(Capabilities.playerType == "Desktop") return true; else return false;		}			}}