/*This class holds the tweets properties*/package com.tweetcatcha.data.objects {		public class Tweet {				//properties		public var tweetId:Number;		public var userId:Number;		public var user:String;		public var profileImage:String;		public var createdTime:String;		public var tweetText:String;		public var parentID:int;		public var ID:int;		public var hour			:int = -1;		public var minute		:int = -1;		public var second		:int = -1;		public var month		:int = -1;		public var day			:int = -1;		public var year			:int = -1;				//constructor		public function Tweet(							  tweetId:Number,							  userId:Number,							  user:String,							  profileImage:String,							  createdTime:String,							  tweetText:String,							  parentID:int,							  ID:int							  ) {			this.tweetId = tweetId;			this.userId = userId;			this.user = user;			this.profileImage = profileImage;			this.createdTime = createdTime;			this.tweetText = tweetText;			this.parentID = parentID;			this.ID = ID;						_parseDate();		}				//parse date		private function _parseDate():void {			var dArr:Array = createdTime.split(" ");			var dateArr:Array = String(dArr[0]).split("-");			var timeArr:Array = String(dArr[1]).split(":");														 			hour = int(timeArr[0]);			minute = int(timeArr[1]);			second = int(timeArr[2]);						month = int(dateArr[1]);			day = int(dateArr[2]);			year = int(dateArr[0]);		}							}}