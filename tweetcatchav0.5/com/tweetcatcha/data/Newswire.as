/**//*This class loads all the news items from newswire, creates and returns the objects*/package com.tweetcatcha.data {		import flash.net.URLLoader;	import flash.net.URLRequest;	import flash.events.Event;	import flash.display.MovieClip;	import flash.events.IOErrorEvent;	import flash.events.EventDispatcher;		import com.tweetcatcha.data.objects.NewsItem;	import com.tweetcatcha.data.objects.Tweets;		import com.tweetcatcha.events.TweetIDEvent;		public class Newswire extends EventDispatcher {		public static const LOADED	:String = "xmlIsLoaded";				private var _idIncrement = 0;				//variables for the xml		private var _xml:XML;				//newsitem		//private var newsItem:NewsItem;		private var newsItems:Array = [];						//constructor		public function Newswire(_xml:XML) {			this._xml = new XML(_xml);		}				public function load():void {			//trace(_xml);			parseXML();		}				//parse xml and create newsitem objects		public function parseXML():void {			var allItems:XMLList = _xml.news_item;			//trace("this is the items" + allItems.length());			for (var i:uint = 0; i < allItems.length(); i++) {				var _headline = allItems[i].headline;				var _url = allItems[i].@url;				var _section = allItems[i].section;				var _summary = allItems[i].summary;				var _updatedTime = allItems[i].updated;				var _createdTime = allItems[i].created;				var _pubDate = allItems[i].pubdate;				var _byLine = allItems[i].byline;				var _tweetsXML:XML = new XML(allItems[i].tweets);								//create a news item object				var newsItem:NewsItem = new NewsItem(_headline, 													 _url, 													 _section, 													 _summary, 													 _updatedTime,													 _createdTime,													 _pubDate,													 _byLine,													 _idIncrement													 );								//push the object into the array				//newsItems[newsItem.ID] = newsItem;				newsItems[newsItem.ID] = newsItem;				//trace(newsItems[newsItem.ID].ID + " " + newsItem.headline);				//trace("these are the tweets" + _tweetsXML.entry.length());				//get and add the tweets to the newsitem				var tweets:Tweets = new Tweets(i, _tweetsXML);				tweets.addEventListener(TweetIDEvent.TWEETID, _onTweetsLoadedForId, false, 0, true);				tweets.load();				newsItems[newsItem.ID].addTweets(tweets.getTweets);				//increment the id								_idIncrement++;			}			dispatchEvent( new Event(LOADED) );		}				private function _onTweetsLoadedForId($e:TweetIDEvent):void {			trace("something?" + $e.getID);			dispatchEvent( $e );		}				//return the objects		public function get getNewsItems():Array {			return newsItems;		}			}}