package com.NickHardeman.loaders {	import flash.events.Event;		public class LoadingImageItemEvent extends Event {		public static const ITEM_COMPLETE:String = "item_loading_complete";				public var name:String;		public var id:*;				public function LoadingImageItemEvent(name:String, bubbles:Boolean=true, cancelable:Boolean=false ) {			super(name, bubbles, cancelable);			this.name = name;		}				public function setInfo(id:*):void {			this.id = id;		}				public override function clone():Event {			var LIE:LoadingImageItemEvent = new LoadingImageItemEvent(name, bubbles, cancelable);			LIE.setInfo(id);			return LIE;		}				public override function toString():String {			return this.name+" id: "+id+" "+super.toString();		}	}}