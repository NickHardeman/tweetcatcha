/*Class that defines a newsitems properties*/package com.tweetcatcha.data.objects {		public class NewsItem {				//properties		public var headline:String;		public var url:String;		public var section:String;		public var summary:String;		public var updatedTime:String;		public var createdTime:String;		public var pubDate:String;		public var ID:int;		public var hour			:int = -1;		public var minute		:int = -1;		public var second		:int = -1;		public var month		:int = -1;		public var day			:int = -1;		public var year			:int = -1;				private var _tweets		:Array = new Array();		public var totalTweets	:int = -1;				//constructor		public function NewsItem(									 headline:String, 									 url:String,									 section:String,									 summary:String,									 updatedTime:String,									 createdTime:String,									 pubDate:String,									 ID:int									 ) {			this.headline = headline;			this.url = url;			this.section = section;			this.summary = summary;			this.updatedTime = updatedTime;			this.createdTime = createdTime;			this.pubDate = pubDate;			this.ID = ID;						_parseDate();		}				private function _parseDate():void {			var dArr:Array = updatedTime.split(" ");			var dateArr:Array = String(dArr[0]).split("-");			var timeArr:Array = String(dArr[1]).split(":");														 			hour = int(timeArr[0]);			minute = int(timeArr[1]);			second = int(timeArr[2]);						month = int(dateArr[1]);			day = int(dateArr[2]);			year = int(dateArr[0]);		}				public function addTweets($t:Array):void {			for (var i:int = 0; i < $t.length; i ++) {				_tweets.push( $t[i] );			}			if (_tweets.length > 0) totalTweets = _tweets.length;			else totalTweets = 0;		}				public function get tweets():Array {			return _tweets;		}			}}