package com.tweetcatcha.data.objects {		import flash.display.MovieClip;		public class NewsItem extends MovieClip {				//properties		public var headline:String;		public var url:String;		public var section:String;		public var summary:String;		public var updatedTime:String;		public var createdTime:String;		public var pubDate:String;		public var ID:int;				//constructor		public function NewsItem(									 headline:String, 									 url:String,									 section:String,									 summary:String,									 updatedTime:String,									 createdTime:String,									 pubDate:String,									 ID:int									 ) {			this.headline = headline;			this.url = url;			this.section = section;			this.summary = summary;			this.updatedTime = updatedTime;			this.createdTime = createdTime;			this.pubDate = pubDate;			this.ID = ID;		}			}}