/*This class holds the tweets properties*/package com.tweetcatcha.data.objects {		import flash.display.MovieClip;		public class Tweet extends MovieClip {				//properties		public var tweetId:Number;		public var userId:Number;		public var user:String;		public var profileImage:String;		public var createdTime:String;		public var tweetText:String;		public var parentID:int;		public var ID:int;				//constructor		public function Tweet(							  tweetId:Number,							  userId:Number,							  user:String,							  profileImage:String,							  createdTime:String,							  tweetText:String,							  parentID:int,							  ID:int							  ) {			this.tweetId = tweetId;			this.userId = userId;			this.user = user;			this.profileImage = profileImage;			this.createdTime = createdTime;			this.tweetText = tweetText;			this.parentID = parentID;			this.ID = ID;		}							}}