package com.tweetcatcha.visual {	import flash.display.MovieClip;		import com.tweetcatcha.data.objects.Tweet;		import com.caurina.transitions.*;		public class TweetManager {				private var _mc			:MovieClip;		private var _tweets		:Vector.<TweetClip>;				public function setup( ):void {			_tweets = new Vector.<TweetClip>();		}				public function addTweet($tarX:Number, $tarY:Number, $t:Tweet):void {			var tc:TweetClip = new TweetClip();			tc.x = 0;			tc.y = 0;			_tweets.push( tc );			Tweener.addTween(tc, {x:$tarX, y:$tarY, time:1, transition:"easeOutCubic"});		}				public function set mc($mc:MovieClip):void {			_mc = $mc;		}	}}