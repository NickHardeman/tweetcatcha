package com.tweetcatcha {		import flash.display.MovieClip;	import flash.events.*;		import com.tweetcatcha.data.Data;		import com.tweetcatcha.visual.Dreamweaver;		public class TweetCatcha extends MovieClip {				//object to load all the data		private var theData:Data;		private var DW				:Dreamweaver;		//object to hold all news items		private var newsItems:Array = [];		//object to hold all tweets		private var tweets:Array = [];				//constructor		public function TweetCatcha() {			//get it up and running			theData = new Data();			DW = new Dreamweaver();			DW.mc = dreamweaver_mc;			theData.loadData();			theData.addEventListener(Data.NEWS_ITEMS_LOADED, getNewsItems);			theData.addEventListener(Data.TWEETS_LOADED, getTweets);		}				//get the news items once the data is loaded		public function getNewsItems($e:Event):void {			//get the news items			newsItems = theData.getNewsItems;			trace("the getNewsItems function is running " +newsItems.length);						for (var i:uint = 0; i < newsItems.length; i++) {				trace(newsItems[i].ID + ": " + newsItems[i].headline);			}						DW.setData(theData);			DW.setup();						DW.addEventListener(MouseEvent.CLICK, _onDWHeadlineClick, false, 0, true);						_onHeadlineClick(30);					}				//get tweets		public function getTweets($e:Event):void {			//get the tweets			tweets = theData.getTweets;						for (var i:uint = 0; i < tweets.length; i++) {				//get the stuff inside there				trace(tweets[i]);			}					}			public function _onDWHeadlineClick($e:MouseEvent = null):void {			//trace($e.target);			_onHeadlineClick( DW.activeID );		}				public function _onHeadlineClick( $id:int ):void {			DW.headlineSelected( $id );		}					}}