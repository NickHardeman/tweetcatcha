package com.tweetcatcha {		import flash.display.MovieClip;	import flash.events.*;		import com.tweetcatcha.data.Data;		public class TweetCatcha extends MovieClip {				//object to load all the data		private var theData:Data;		//object to hold all news items		private var newsItems:Array = [];				//constructor		public function TweetCatcha() {			//get it up and running			theData = new Data();			theData.loadData();			theData.addEventListener(Data.NEWS_ITEMS_LOADED, getNewsItems);		}				//get the news items once the data is loaded		public function getNewsItems($e:Event):void {			//get the news items			newsItems = theData.getNewsItems;			//trace(newsItems.length);						for (var i:uint = 0; i < newsItems.length; i++) {				trace(newsItems[i].ID + ": " + newsItems[i].headline);			}					}					}}